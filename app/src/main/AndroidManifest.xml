<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.alperez.listbook2010example">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ListBook2010Example">
        <activity
            android:name=".mainactivity.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".staticdemo.StaticDemoActivity"></activity>
        <activity android:name=".dynamicdemo.DynamicDemoActivity"></activity>
        <activity android:name=".recyclingdemo.RecyclingDemoActivity"></activity>
        <activity android:name=".ratelistdemo.RateListDemoActivity"></activity>
    </application>

</manifest>